    },
    "PackageManagerConfiguration": {
        "desiredState": {
            "gpgKeys": {
                "pkgs-msft_key": "https://packages.microsoft.com/keys/microsoft.asc"
            },
            "sources": {
                "pkgs-msft_insiders-slow": "deb [arch=amd64,armhf,arm64 signed-by=pkgs-msft_key] https://packages.microsoft.com/ubuntu/20.04/prod insiders-slow main"
            },
            "packages": [
                "cowsay",
                "procdump"
            ]
        }
    }
}
